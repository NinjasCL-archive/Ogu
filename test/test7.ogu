;; test 7 many functions

let factorial n = if n == 0 then 1 else n * factorial (n-1)

let min'(a, b)  = if a < b then a else b

let swap'(a, b)  = (b,a)

let min(a,b) = if a < b then a else b

let swap(a:Num, b:Num) = (b,a)

let sumarVectores (a:Num,b:Num) (c:Num,c:Num) = (a+c, b+d)

let sumarVectores (a,b:Num) (c,d:Num) = (a+c, b+d)

let test =
    sumarVectores (10,10) (20,20) ; produce (30,30)

