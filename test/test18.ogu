trait YesNo a where

  def yesno : a -> Bool

instance YesNo Int where
  let yesno 0 = false
  let yesno _ = true

instance YesNo [a] where

  let yesno [] = false
  let yesno _ = true

instance YesNo Bool where

  let yesno = id ;; Bool tiene una funci√≥n id

data Maybe s = Nothing | Some(s)

instance YesNo Maybe T where
  let yesno Just(_) = true
  let yesno Nothing = False

