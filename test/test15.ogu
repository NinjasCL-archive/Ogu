// classes initialization


class Stack() = {
    var _data : [Int] = []

    def push! self x:Int do _data = cons x _data

   def pop! self = 
           let result = head _data in {
           set _data = tail _data // assignment begin with set
           result
   }

   def empty? self = {
           empty _data
   }
}

def test'() {
  var stack := Stack()  
  push! stack 10
  push! stack 20
  pop! stack /// <- retorna 20

  val auto := Auto()
    auto.bencina

}


