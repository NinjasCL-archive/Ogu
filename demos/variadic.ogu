
def greet = greet "you"

def greet name
    | name == "world " = println! "hello world"
    | otherwise = println! "hello " name

def greet name1 name2 = println! "hello " name1 " & " name2


(greet)

greet "world"

greet "Mampato"

greet "Mampato" "Rena"


def greet2 name rest... =  println! $ str "hola " name ", " (join ", " rest)


greet2 "Mampato" "Ogu" "Rena" "Xse"
