

greet = greet "you"

greet name
    | name == "world " = println! "hello world"
    | otherwise = println! "hello " name

greet name1 name2 = println! "hello " name1 " & " name2


(greet)

greet "world"

greet "Mampato"

greet "Mampato" "Rena"


greet2 name & rest = apply  prn  ("hola " :: (name :: rest))


greet2 "Mampato" "Ogu" "Rena" "Xse"
