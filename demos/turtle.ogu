use ogu.turtle

def fib turtle depth = begin
  forward turtle 30
  when depth > 2 do
  begin
        left turtle 15
    fib turtle (depth - 1)
    right turtle 30
    fib turtle (depth - 2)
    left turtle 15
  end
  back turtle 30
end


turtle 400 400
    !> pen-up
    !> go 0 -100
    !> pen-down
    !> fib 10
    !> show