def determinar-ganancia mano-jugador mano-croupier apuesta jugada
  | jugada == 'R' = neg apuesta
  | jugada == 'D' = 2 * ganancia
  | otherwise = ganancia
  where (jugador, croupier) = (valor-mano? mano-jugador, valor-mano? mano-croupier)
         ganancia = if (croupier <= 21 && jugador > 21) || (jugador < croupier && croupier <= 21)  then neg apuesta else apuesta
